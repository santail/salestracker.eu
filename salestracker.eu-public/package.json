  "main": "webpack.config.js",
  "scripts": {
    "start": "node server",
    "start:server": "ts-node src/server",
    "dev:server": "nodemon -e ts --ignore \"src/client/**/*\" --ignore \"src/server/tests/**/*\"  --exec \"npm run start:server\"",
    "dev:client": "webpack -wd",
    "test": "mocha -r ts-node/register -r jsdom-global/register -r src/client/tests/configureTests \"src/client/tests/**/*.*\" \"src/server/tests/**/*.*\"",
    "test:watch": "npm run test -- --watch-extensions ts,tsx --watch",
    "dev": "npm-run-all --parallel dev:server dev:client"
  },
